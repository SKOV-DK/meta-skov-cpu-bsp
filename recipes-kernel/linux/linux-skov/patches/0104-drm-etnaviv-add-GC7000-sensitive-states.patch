@@ -82,6 +82,17 @@ static const struct {
 	ST(0x10800, 32 * 16),
 	ST(0x14600, 16),
 	ST(0x14800, 8 * 8),
+	ST(0x14000, 1),
+	ST(0x14010, 1),
+	ST(0x14018, 1),
+	ST(0x14020, 1),
+	ST(0x14074, 1),
+	ST(0x1407c, 1),
+	ST(0x14084, 1),
