From: Juergen Borleis <jbe@pengutronix.de>
Date: Wed, 2 Dec 2015 13:58:20 +0100
Subject: [PATCH] DTS: configure output GPIOs correctly

Signed-off-by: Juergen Borleis <jbe@pengutronix.de>
---
 arch/arm/boot/dts/imx6qdl-cpu.dtsi  | 11 +++++++----
 arch/arm/boot/dts/imx6qdl-cpuB.dtsi |  2 +-
 arch/arm/boot/dts/imx6qdl-cpuC.dtsi |  4 ++--
 3 files changed, 10 insertions(+), 7 deletions(-)

diff --git a/arch/arm/boot/dts/imx6qdl-cpu.dtsi b/arch/arm/boot/dts/imx6qdl-cpu.dtsi
index d62efafaceff..0136161cf77c 100644
--- a/arch/arm/boot/dts/imx6qdl-cpu.dtsi
+++ b/arch/arm/boot/dts/imx6qdl-cpu.dtsi
@@ -362,7 +362,7 @@
 
 		pinctrl_io_vcc_mmc: vcc_io_mmc_grp {
 			fsl,pins = <
-				MX6QDL_PAD_GPIO_18__GPIO7_IO13		0x17059		/* MMC IO Supply Select */
+				MX6QDL_PAD_GPIO_18__GPIO7_IO13		0x40000058	/* MMC IO Supply Select */
 			>;
 		};
 
@@ -371,7 +371,8 @@
 				MX6QDL_PAD_EIM_D17__ECSPI1_MISO		0x100b1
 				MX6QDL_PAD_EIM_D18__ECSPI1_MOSI		0x100b1
 				MX6QDL_PAD_EIM_D16__ECSPI1_SCLK		0x100b1
-				MX6QDL_PAD_EIM_D24__GPIO3_IO24		0x100b1		/* spi-nor CS */
+				/* *no* external pull up */
+				MX6QDL_PAD_EIM_D24__GPIO3_IO24		0x40000058	/* spi-nor CS */
 			>;
 		};
 
@@ -380,7 +381,8 @@
 				MX6QDL_PAD_EIM_OE__ECSPI2_MISO		0x100b1
 				MX6QDL_PAD_EIM_CS1__ECSPI2_MOSI		0x100b1
 				MX6QDL_PAD_EIM_CS0__ECSPI2_SCLK		0x100b1
-				MX6QDL_PAD_EIM_RW__GPIO2_IO26		0x100b1		/* mcp2003 CS */
+				/* external pull up */
+				MX6QDL_PAD_EIM_RW__GPIO2_IO26		0x40000058	/* mcp2003 CS */
 			>;
 		};
 
@@ -389,7 +391,8 @@
 				MX6QDL_PAD_EIM_D28__ECSPI4_MOSI 	0x100b1
 				MX6QDL_PAD_EIM_D22__ECSPI4_MISO		0x100b1
 				MX6QDL_PAD_EIM_D21__ECSPI4_SCLK		0x100b1
-				MX6QDL_PAD_EIM_D20__GPIO3_IO20		0x100b1		/* TSC2046 CS */
+				/* *no* external pull up */
+				MX6QDL_PAD_EIM_D20__GPIO3_IO20		0x40000058	/* TSC2046 CS */
 			>;
 		};
 
diff --git a/arch/arm/boot/dts/imx6qdl-cpuB.dtsi b/arch/arm/boot/dts/imx6qdl-cpuB.dtsi
index 3494e5be93f4..45f560b71fbd 100644
--- a/arch/arm/boot/dts/imx6qdl-cpuB.dtsi
+++ b/arch/arm/boot/dts/imx6qdl-cpuB.dtsi
@@ -27,7 +27,7 @@
 
 	pinctrl_backlight: backlight_grp {
 		fsl,pins = <
-			MX6QDL_PAD_RGMII_TD3__GPIO6_IO23	0x1b0b0
+			MX6QDL_PAD_RGMII_TD3__GPIO6_IO23	0x40000058
 		>;
 	};
 };
diff --git a/arch/arm/boot/dts/imx6qdl-cpuC.dtsi b/arch/arm/boot/dts/imx6qdl-cpuC.dtsi
index 441d57e4b2cb..36d6222211ef 100644
--- a/arch/arm/boot/dts/imx6qdl-cpuC.dtsi
+++ b/arch/arm/boot/dts/imx6qdl-cpuC.dtsi
@@ -27,13 +27,13 @@
 
 	pinctrl_backlight: backlight_grp {
 		fsl,pins = <
-			MX6QDL_PAD_RGMII_TD3__GPIO6_IO23	0x1b0b0
+			MX6QDL_PAD_RGMII_TD3__GPIO6_IO23	0x40000058
 		>;
 	};
 
 	pinctrl_vcc_mmc: vcc_mmc_grp {
 		fsl,pins = <
-			MX6QDL_PAD_SD3_RST__GPIO7_IO08		0x1b0b0		/* MMC Power Supply Switch */
+			MX6QDL_PAD_SD3_RST__GPIO7_IO08		0x40000058	/* MMC Power Supply Switch */
 		>;
 	};
 };
